<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.anonym.spring.mapper.UserShopToShopMapper">


    <select id="selectIsExtis" resultType="java.lang.String">
