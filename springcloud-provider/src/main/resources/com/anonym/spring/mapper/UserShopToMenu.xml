<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.anonym.spring.mapper.UserShopToMenu">

    <select id="selectMenuIds" resultType="java.util.List">
        select menuId from usershop_to_menu
        where shopId = #{shopId}
    </select>

</mapper>